{
    "name": "rmcgirr83/genders",
    "type": "phpbb-extension",
    "description": "This MOD will allow your members to specify their Gender. They can chose between Male, Female and None specified.",
    "version": "1.0.1",
    "time": "2016-03-22",
    "keywords": [
        "phpbb",
        "extension",
        "genders",
        "male",
        "female",
        "communication"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Rich McGirr",
            "email": "rmcgirr83@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.4-RC1,<3.2.0"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Genders",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.4-RC1,<3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/phpbb_3.1_genders",
            "filename": "version_check"
        }
    }
}